{% extends "AppBundle::layoutAdmin.html.twig" %}

{% block title %}Supprimer une fiche{% endblock %}

{% block body %}
<h2>Supprimer une fiche</h2>
<p>
    Etes-vous certain de vouloir supprimer la fiche "#{{ taxrefv10.id }} - {{ taxrefv10.LbNom }}" ?
</p>
{{ form_start(form) }}
<a href="{{ path('admin') }}" class="btn btn-default">Retour au tableau de bord</a>
<input type="submit" value="Supprimer" class="btn btn-danger" />
{{ form_end(form) }}
{% endblock %}